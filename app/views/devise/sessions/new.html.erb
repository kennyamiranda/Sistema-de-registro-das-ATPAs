<div class="container">
  <div class="panel panel_header">
      <div class="panel-head">
        <h2>Sistema de registro das Atividades Teórico-Práticas de</h2>
        <div class="aprofundamento">
          <h2>Aprofundamento (ATPA)</h2>
        </div>
      </div>
  </div>

  <!-- //Início do form para identificação do login para as credenciais de email e senha -->
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>

    <div class="field">
      <%= f.label :password %><br />
      <%= f.password_field :password, autocomplete: "Senha" %>
    </div>

    <div class="button_session">
      <div class="actions">
        <%= f.submit "Entrar" %>
      </div>
    </div>
    <% end %>

    <%= render "devise/shared/links" %>
     <%= link_to "Voltar", welcome_index_path, :class => "btn btn-primary" %>

    <style type="text/css">
      .container ul li {
        margin-top:-5px;
        margin-left:-40px;
      }
    </style>
</div>
